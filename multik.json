{
  "dependencies": {
    "core": {
      "group": "core",
      "repo": "https://github.com/abricos/abricos-core.git",
      "version": "api"
    },
    "install": {
      "group": "installer",
      "repo": "https://github.com/abricos/abricos-installer.git",
      "version": "master"
    },
    "abricos.js": {
      "group": "vendor",
      "repo": "https://github.com/abricos/abricos.js.git",
      "version": "master"
    },
    "bos": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-bos.git",
      "version": "master"
    },
    "example": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-example.git",
      "version": "master"
    },
    "filemanager": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-filemanager.git",
      "version": "master"
    },
    "notify": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-notify.git",
      "version": "api"
    },
    "sitemap": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-sitemap.git",
      "version": "master"
    },
    "tinymce": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-tinymce.git",
      "version": "master"
    },
    "uprofile": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-uprofile.git",
      "version": "master"
    },
    "user": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-user.git",
      "version": "api"
    },
    "widget": {
      "group": "module",
      "repo": "https://github.com/abricos/abricos-mod-widget.git",
      "version": "master"
    }
  },
  "directory": "abricos.src",
  "groups": {
    "core": {
      "directory": "abricos.src"
    },
    "installer": {
      "directory": "abricos.src"
    },
    "module": {
      "directory": "abricos.src/modules"
    },
    "template": {
      "directory": "abricos.src/templates"
    },
    "vendor": {
      "directory": "abricos.src/vendor"
    }
  }
}
